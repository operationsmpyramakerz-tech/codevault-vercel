{
  "version": 2,
  "rewrites": [
    {
      "source": "/",
      "destination": "/login.html"
    },
    {
      "source": "/login",
      "destination": "/login.html"
    },
    {
      "source": "/logout",
      "destination": "/login.html"
    },
    {
      "source": "/dashboard",
      "destination": "/index.html"
    },
    {
      "source": "/orders",
      "destination": "/index.html"
    },
    {
      "source": "/orders/requested",
      "destination": "/requested_orders.html"
    },
    {
      "source": "/orders/assigned",
      "destination": "/assigned_schools_requested_orders.html"
    },
    {
      "source": "/orders/new",
      "destination": "/create_new_order.html"
    },
    {
      "source": "/stock",
      "destination": "/stocktaking.html"
    },
    {
      "source": "/funds",
      "destination": "/funds.html"
    },
    {
      "source": "/account",
      "destination": "/account.html"
    },
    {
      "source": "/orders/:path*",
      "destination": "/index.html"
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}