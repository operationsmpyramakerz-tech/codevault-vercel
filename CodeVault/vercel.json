{
  "version": 2,
  "cleanUrls": true,
  "trailingSlash": false,
  "routes": [
    {
      "src": "^/$",
      "dest": "/public/login.html"
    },
    {
      "src": "^/login/?$",
      "dest": "/public/login.html"
    },
    {
      "src": "^/logout/?$",
      "dest": "/public/login.html"
    },
    {
      "src": "^/orders/?$",
      "dest": "/public/index.html"
    },
    {
      "src": "^/current-orders/?$",
      "dest": "/public/index.html"
    },
    {
      "src": "^/orders/requested/?$",
      "dest": "/public/requested-orders.html"
    },
    {
      "src": "^/orders/assigned/?$",
      "dest": "/public/assigned-orders.html"
    },
    {
      "src": "^/orders/new/?$",
      "dest": "/public/create-order-details.html"
    },
    {
      "src": "^/orders/new/products/?$",
      "dest": "/public/create-order-products.html"
    },
    {
      "src": "^/orders/new/review/?$",
      "dest": "/public/create-order-review.html"
    },
    {
      "src": "^/stocktaking/?$",
      "dest": "/public/stocktaking.html"
    },
    {
      "src": "^/funds/?$",
      "dest": "/public/funds.html"
    },
    {
      "src": "^/account/?$",
      "dest": "/public/account.html"
    },
    {
      "src": "^/(css|js|images|fonts)/(.*)$",
      "dest": "/public/$1/$2"
    }
  ]
}