{
  "version": 2,
  "rewrites": [
    {
      "source": "/",
      "destination": "/public/index.html"
    },
    {
      "source": "/login",
      "destination": "/public/login.html"
    },
    {
      "source": "/dashboard",
      "destination": "/public/index.html"
    },
    {
      "source": "/orders/requested",
      "destination": "/public/requested-orders.html"
    },
    {
      "source": "/orders/assigned",
      "destination": "/public/assigned-orders.html"
    },
    {
      "source": "/orders/new",
      "destination": "/public/create-order-details.html"
    },
    {
      "source": "/orders/new/products",
      "destination": "/public/create-order-products.html"
    },
    {
      "source": "/orders/new/review",
      "destination": "/public/create-order-review.html"
    },
    {
      "source": "/stock",
      "destination": "/public/stocktaking.html"
    },
    {
      "source": "/funds",
      "destination": "/public/funds.html"
    },
    {
      "source": "/account",
      "destination": "/public/account.html"
    },
    {
      "source": "/debug-login",
      "destination": "/public/debug-login.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)\\.(css|js|png|jpg|jpeg|gif|svg|webp|ico|woff|woff2)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}